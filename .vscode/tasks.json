{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "coreui start",
      "type": "shell",
      "command": "npm start",
      "isBackground": true,
      "options": {
        "cwd": "${workspaceFolder}/coreui"
      },
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "group": "frontend",
        "panel": "shared",
        "showReuseMessage": true,
        "clear": false
      },
      "problemMatcher": [
        {
          "pattern": [
            {
              "regexp": ".",
              "file": 1,
              "location": 2,
              "message": 3
            }
          ],
          "background": {
            "activeOnStart": true,
            "beginsPattern": ".",
            "endsPattern": "watching for file changes..."
          }
        }
      ]
    },
    {
      "label": "app start",
      "type": "shell",
      "command": "npm start",
      "dependsOn": ["coreui start"],
      "options": {
        "cwd": "${workspaceFolder}/app"
      },
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "group": "frontend",
        "panel": "new",
        "showReuseMessage": true,
        "clear": false
      },
      "problemMatcher": []
    }
  ]
}
