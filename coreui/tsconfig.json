{
  "extends": "../scaffold/tsconfig.common.json",
  "compilerOptions": {
    "allowJs": false,
    "declarationDir": "lib",
    "isolatedModules": true,
    "jsx": "react-jsx",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "module": "esnext",
    "noEmit": true,
    "resolveJsonModule": true,
    "rootDir": "src",
    "skipLibCheck": false,
    "target": "ESNext",
    "types": ["vite/client"]
  },
  "include": ["src"]
}
